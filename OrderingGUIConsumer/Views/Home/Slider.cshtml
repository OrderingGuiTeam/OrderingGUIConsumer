
@{
    Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Metallic Slider - Free Website Template</title>
    <meta name="keywords" content="#" />
    <meta name="description" content="#" />
    <!--
    Template 2058 metallic slider
    http://www.tooplate.com/view/2058-metallic-slider
    -->
    <link href="~/Content/tooplate_style.css" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/jquery-1.2.6.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.easing.1.3.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.kwicks-1.5.1.pack.js" type="text/javascript"></script>

    <script type="text/javascript">
	$().ready(function() {
		$('.kwicks').kwicks({
			max : 696,
			spacing : 0,
			sticky: true,
		});
	});
    </script>

    <script type="text/javascript" src="~/Scripts/jquery-1-4-2.min.js"></script>
    <link rel="stylesheet" href="~/Content/slimbox2.css" type="text/css" media="screen" />
    <script type="text/JavaScript" src="~/Scripts/slimbox2.js"></script>

</head>

<body>
    <div id="bar"><br><font color="white"><h1>Place an Order</h1></font></div>
    <div id="tooplate_main">

        <div id="tooplate_content">

            <ul class="kwicks">

                <li id="home">
                    <span class="header"></span><span class="slideno"></span>

                    <div class="inner">
                        <h2><font color="red">User Login</font></h2>
                        <p></p>
                        <h4><font color="red">Enter the Customer ID or SSN</font></h4>
                        <div id="contact_form">
                            <form method="post" name="contact" action="#">

                                <label for="author">Customer ID:</label> <input type="text" id="author" name="author" class="required input_field" />
                                <div class="cleaner_h10"></div>

                                <input type="submit" value="Validate" id="submit" name="submit" class="submit_btn float_l" />

                            </form>
                        </div>
                    </div>
                </li>

                <li id="about">
                    <span class="header"></span><span class="slideno"></span>
                    <div class="inner">
                        <h2><font color="red">Our Services</font></h2>

                        <ul class="social_links">
                            <li><img src="~/images/tv1.jpg" alt="Image 02" class="image_fr" height="100" width="100" /></li>
                            <li><img src="~/images/data.jpg" alt="Image 02" class="image_fr" height="100" width="100" /></li>
                            <li><img src="~/images/tv3.jpg" alt="Image 02" class="image_fr" height="100" width="100" /> </li>
                        </ul>

                    </div>
                </li>

                <li id="social">
                    <span class="header"></span><span class="slideno"></span>
                    <div class="inner">
                        <h2><font color="red">Selecting Bundles or Products</font></h2>
                        <p><em>Select bundles or the Products</em></p>
                        <ul class="social_links">
                            <!--
                            <li><a href="images/portfolio/01-l.jpg" class="image_fr" rel="lightbox[portfolio]" ><img src="images/portfolio/Capture1.png" alt="Image 01" /></a></li> -->
                            <li><img src="~/images/portfolio/Capture1.png" width="150" height="150" alt="Image 01" /></li>
                            <li><img src="~/images/portfolio/Capture2.png" width="150" height="150" alt="Image 02" /></li>
                            <li><img src="~/images/portfolio/Capture3.png" width="150" height="150" alt="Image 03" /></li>
                            <li><img src="~/images/portfolio/Capture4.png" width="150" height="150" alt="Image 04" /></li>
                            <li><img src="~/images/portfolio/Capture5.png" width="150" height="150" alt="Image 05" /></li>
                            <li><img src="~/images/portfolio/Capture6.png" width="150" height="150" alt="Image 06" /></li>

                        </ul>


                    </div>
                </li>

                <li id="portfolio">
                    <span class="header"></span><span class="slideno"></span>
                    <div class="inner">

                        <h2><font color="red">Order Confirmation</font></h2>
                        <p></p>

                        <div id="contact_form">
                            <form method="post" name="contact" action="#">

                                <label for="author">Customer ID:</label>
                                <div class="cleaner_h10"></div>
                                <label for="author">First Name:</label>
                                <div class="cleaner_h10"></div>
                                <label for="author">Last Name:</label>
                                <div class="cleaner_h10"></div>
                                <label for="email">Email:</label>
                                <div class="cleaner_h10"></div>

                                <label for="text">State:</label>
                                <div class="cleaner_h10"></div>
                                <label for="text">Packages Name:</label>
                                <div class="cleaner_h10"></div>
                                <label for="text">Product Name:</label>
                                <div class="cleaner_h10"></div>
                                <label for="text">Bundle Type:</label>
                                <div class="cleaner_h10"></div>
                                <label for="text">Bundle Description:</label>
                                <div class="cleaner h10"></div>
                                <label for="text">Bundle Level:</label>
                                <div class="cleaner h10"></div>

                                <input type="submit" value="Send" id="submit" name="submit" class="submit_btn float_l" />
                                <input type="reset" value="Reset" id="reset" name="reset" class="submit_btn float_r" />
                            </form>
                        </div>
                    </div>
                </li>

                <li id="contact">
                    <span class="header"></span><span class="slideno"></span>
                    <div class="inner">

                    </div>


                </li>

            </ul>

        </div> <!-- END of content -->
    </div> <!-- END of tooplate_main -->

</body>
</html>